<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>AddProduct</servlet-name>
        <servlet-class>AddProduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddressForm</servlet-name>
        <servlet-class>AddressForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CancelOrder</servlet-name>
        <servlet-class>CancelOrder</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FeedbackSolution</servlet-name>
        <servlet-class>FeedbackSolution</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ordersreport</servlet-name>
        <servlet-class>ordersreport</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddProduct</servlet-name>
        <url-pattern>/AddProduct</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddressForm</servlet-name>
        <url-pattern>/AddressForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CancelOrder</servlet-name>
        <url-pattern>/CancelOrder</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FeedbackSolution</servlet-name>
        <url-pattern>/FeedbackSolution</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ordersreport</servlet-name>
        <url-pattern>/ordersreport</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
